{
	"files.exclude": {
		"**/node_modules": true,
		"**/out": true,
		"**/.gitattributes": true,
		"**/.gitignore": true,
		"**/package-lock.json": true,
		"**/tsconfig*.json": true,
		"**/tslint.json": true,
		"**/*.vsix": true
	},
	"editor.insertSpaces": false,
	"tslint.enable": true,
	"typescript.tsc.autoDetect": "off",
	"typescript.preferences.quoteStyle": "single"
}